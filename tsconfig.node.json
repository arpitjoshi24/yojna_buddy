{
  "compilerOptions": {
    "target": "ES2020",
    "module": "ESNext",
"moduleResolution": "NodeNext",
    "outDir": "./dist",
    "rootDir": "./server",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "allowJs": true
  },
  "ts-node": {
    "esm": true
  },
  "include": ["server/**/*.ts"],
  "exclude": ["node_modules"]
}
